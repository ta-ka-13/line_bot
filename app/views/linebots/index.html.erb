<p id="notice"><%= notice %></p>

<h1>Linebots</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @linebots.each do |linebot| %>
      <tr>
        <td><%= linebot.title %></td>
        <td><%= linebot.body %></td>
        <td><%= link_to 'Show', linebot %></td>
        <td><%= link_to 'Edit', edit_linebot_path(linebot) %></td>
        <td><%= link_to 'Destroy', linebot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Linebot', new_linebot_path %>
